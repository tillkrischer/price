{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:import/typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["build"],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
